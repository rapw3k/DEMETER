                                       ] .

#################################################################
#    Annotation properties
#################################################################


#################################################################
#    Datatypes
#################################################################


#################################################################
#    Object Properties
#################################################################

### https://uri.etsi.org/ngsi-ld/Property
ngsi-ld:Property rdf:type owl:ObjectProperty ;
                  rdfs:label "Property" ;
                  rdfs:comment "A relation defining a property and a value associated with an entity" ;
                  rdfs:isDefinedBy <https://uri.etsi.org/ngsi-ld/v1/ngsi-ld-core-context-v1.6.jsonld> ;
								 rdfs:range [ rdf:type owl:Class ;
								 							owl:unionOf (
								 								ngsi-ld:Property
								 								ngsi-ld:Value
								 							)
								 						] .

### https://uri.etsi.org/ngsi-ld/Relationship
ngsi-ld:Relationship rdf:type owl:ObjectProperty ;
                  rdfs:label "Relationship" ;
                  rdfs:comment "Relationship betweeen two entities" ;
                  rdfs:isDefinedBy <https://uri.etsi.org/ngsi-ld/v1/ngsi-ld-core-context-v1.6.jsonld> ;
										 rdfs:domain ngsi-ld:Entity ;
										 rdfs:range [ rdf:type owl:Class ;
										 							owl:unionOf (
										 								ngsi-ld:Entity
										 								ngsi-ld:Relationship
										 							)
										 						] .

### https://uri.etsi.org/ngsi-ld/hasObject
ngsi-ld:hasObject rdf:type owl:ObjectProperty ;
                  rdfs:label "has object" ;
                  rdfs:comment "A realtion betwen a Relationship and an Entity" ;
                  rdfs:isDefinedBy <https://uri.etsi.org/ngsi-ld/v1/ngsi-ld-core-context-v1.6.jsonld> ;
									rdfs:domain ngsi-ld:Relationship ;
									rdfs:range ngsi-ld:Entity .

#################################################################
#    Data properties
#################################################################

### https://uri.etsi.org/ngsi-ld/hasValue
ngsi-ld:hasValue rdf:type owl:DatatypeProperty ;
                rdfs:label "has value" ;
                rdfs:comment "A realtion confirming the obeserved value of an entity" ;
                rdfs:isDefinedBy <https://uri.etsi.org/ngsi-ld/v1/ngsi-ld-core-context-v1.6.jsonld> ;
								rdfs:domain ngsi-ld:Property .


#################################################################
#    Classes
#################################################################

### https://uri.etsi.org/ngsi-ld/Entity
ngsi-ld:Entity rdf:type owl:Class ;
              rdfs:label "Entity" ;
              rdfs:comment "An Entity" ;
              rdfs:isDefinedBy <https://uri.etsi.org/ngsi-ld/v1/ngsi-ld-core-context-v1.6.jsonld> ;
							owl:disjointWith
								ngsi-ld:Property ,
								ngsi-ld:Relationship ,
								ngsi-ld:Value .

### https://uri.etsi.org/ngsi-ld/Property
ngsi-ld:Property rdf:type owl:Class ;
              rdfs:label "Property" ;
              rdfs:comment "A Property" ;
              rdfs:isDefinedBy <https://uri.etsi.org/ngsi-ld/v1/ngsi-ld-core-context-v1.6.jsonld> ;
							owl:disjointWith ngsi-ld:Relationship .

### https://uri.etsi.org/ngsi-ld/Relationship
ngsi-ld:Relationship rdf:type owl:Class ;
              rdfs:label "Relationship" ;
              rdfs:comment "A Relationship class" ;
              rdfs:isDefinedBy <https://uri.etsi.org/ngsi-ld/v1/ngsi-ld-core-context-v1.6.jsonld> ;
							owl:disjointWith ngsi-ld:Value .

### https://uri.etsi.org/ngsi-ld/Value
ngsi-ld:Value rdf:type owl:Class ;
              rdfs:label "Value" ;
              rdfs:comment "A Value class" ;
              rdfs:isDefinedBy <https://uri.etsi.org/ngsi-ld/v1/ngsi-ld-core-context-v1.6.jsonld> .

