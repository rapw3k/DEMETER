    <!-- http://inspire.ec.europa.eu/schemas/af/3.0#FarmAnimalSpecies -->


    <owl:Class rdf:about="http://inspire.ec.europa.eu/schemas/af/3.0#FarmAnimalSpecies">
        <owl:equivalentClass rdf:resource="https://saref.etsi.org/saref4agri/Animal"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://foodie-cloud.com/model/foodie#livestockNumber"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://foodie-cloud.com/model/foodie#livestockType"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Identifies an animal or group of animals of the same species kept on the specific site</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://inspire.ec.europa.eu/schemas/af/3.0#"/>
        <rdfs:label>Farm Animal Species</rdfs:label>
        <skos:prefLabel xml:lang="en">FarmAnimalSpecies</skos:prefLabel>
    </owl:Class>
    


    <!-- https://saref.etsi.org/core/FeatureOfInterest -->


    <owl:Class rdf:about="https://saref.etsi.org/core/FeatureOfInterest"/>
    


    <!-- https://saref.etsi.org/saref4agri/Animal -->


    <owl:Class rdf:about="https://saref.etsi.org/saref4agri/Animal">
        <owl:equivalentClass rdf:resource="https://smartdatamodels.org/dataModel.Agrifood/Animal"/>
        <rdfs:subClassOf rdf:resource="https://saref.etsi.org/core/FeatureOfInterest"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/TAXRANK_1000000"/>
                <owl:someValuesFrom rdf:resource="http://purl.obolibrary.org/obo/TAXRANK_0000000"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://saref.etsi.org/saref4agri/isLocatedIn"/>
                <owl:someValuesFrom rdf:resource="http://www.opengis.net/ont/geosparql#Feature"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://saref.etsi.org/saref4agri/isMemberOf"/>
                <owl:someValuesFrom rdf:resource="https://saref.etsi.org/saref4agri/AnimalGroup"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://saref.etsi.org/saref4agri/hasID"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="https://saref.etsi.org/saref4agri/ID"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://saref.etsi.org/saref4agri/hasBirthDate"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://saref.etsi.org/saref4agri/hasDeathDate"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://saref.etsi.org/saref4agri/hasName"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">An individual and identifiable living organism that feeds on organic matter, typically having specialized sense organs and nervous system and able to respond rapidly to stimuli.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://saref.etsi.org/saref4agri/"/>
        <rdfs:label>Animal</rdfs:label>
    </owl:Class>
    


    <!-- https://saref.etsi.org/saref4agri/AnimalGroup -->


    <owl:Class rdf:about="https://saref.etsi.org/saref4agri/AnimalGroup">
