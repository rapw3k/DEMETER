           rdfs:subPropertyOf <https://w3id.org/saref#hasProperty> ;
           rdfs:comment "A relation to represent the yield generated by a Crop." ;
           rdfs:label "generates"@en ;
           rdfs:isDefinedBy <https://w3id.org/def/saref4agri#> .


