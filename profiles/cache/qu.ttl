    rdfs:comment "A DimensionFactor represents a factor in the product of powers that defines a Dimension." ;
    rdfs:isDefinedBy "http://purl.oclc.org/NET/ssnx/qu" ;
    rdfs:seeAlso "http://www.omgsysml.org/qudv#Dimension" ;
