qu:Prefix a owl:Class ;
    rdfs:label "Prefix" ;
    dc:source "skos:exactMatch 'Prefix' [SysML 1.2-QUDV] http://www.omgwiki.org/OMGSysML/doku.php?id=sysml-qudv:quantities_units_dimensions_values_qudv" ;
