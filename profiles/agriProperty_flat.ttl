    rdfs:isDefinedBy <http://foodie-cloud.com/model/foodie> ;
    rdfs:range foodie:PropertyType ;
    rdfs:subPropertyOf saref:hasProperty ;
    skos:prefLabel "productionProperty"@en .

foodie:soilProperty a owl:ObjectProperty ;
